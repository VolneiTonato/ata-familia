<!DOCTYPE html>
<html lang="en">
    <head>

        <!-- start: Meta -->
        <meta charset="utf-8">
        <title>{% block title %}{{ januxPageTitle }}{% endblock %}</title>
        <meta name="description" content="Bootstrap Metro Dashboard">
        <meta name="author" content="Volnei Tonato">
        <meta name="keyword" content="serviÃ§os hos">
        <!-- end: Meta -->

        <!-- start: Mobile Specific -->
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <!-- end: Mobile Specific -->

        {% block styles %}
            <link rel="stylesheet" id="bootstrap-style" href="{{ asset('bundles/vtttemplatesjanux/css/bootstrap.min.css') }}" />
            <link rel="stylesheet" href="{{ asset('bundles/vtttemplatesjanux/css/bootstrap-responsive.min.css') }}" />
            <link rel="stylesheet" id="base-style" href="{{ asset('bundles/vtttemplatesjanux/css/style.css') }}" />
            <link rel="stylesheet" id="base-style-responsive" href="{{ asset('bundles/vtttemplatesjanux/css/style-responsive.css') }}" />            
            <link rel="stylesheet" href="{{ asset('bundles/vtttemplatesjanux/library/datatable/css/DT_bootstrap.css') }}" />    
            <link href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800&subset=latin,cyrillic-ext,latin-ext' rel='stylesheet' type='text/css'>

            <!--<link rel="shortcut icon" href="" type="image/x-icon" />-->
        {% endblock %}


        <!-- The HTML5 shim, for IE6-8 support of HTML5 elements -->
        <!--[if lt IE 9]>
                <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
                <link rel="stylesheet" href="{{ asset('bundles/vtttemplatesjanux/css/ie.css') }}" />
        <![endif]-->

        <!--[if IE 9]>
                <link rel="stylesheet" id="ie9style" href="{{ asset('bundles/vtttemplatesjanux/css/ie9.css') }}" />
        <![endif]-->

    </head>

    <body>
        <!-- start: Header -->
        <div class="navbar">
            <div class="navbar-inner">
                <div class="container-fluid">
                    <a class="btn btn-navbar" data-toggle="collapse" data-target=".top-nav.nav-collapse,.sidebar-nav.nav-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </a>
                    <a class="brand" href="{{ januxPageLinkTitle }}"><span>{{ januxPageTitle }}</span></a>

                    <!-- start: Header Menu -->
                    <div class="nav-no-collapse header-nav">
                        <ul class="nav pull-right">

                            <!-- start: User Dropdown -->
                            <li class="dropdown">
                                <a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
                                    <i class="halflings-icon white user"></i> 
                                    <span class="caret"></span>
                                </a>
                                <!--
                            <ul class="dropdown-menu">
                                
                                <li class="dropdown-menu-title">
                                    <span>Account Settings</span>
                                </li>
                                <li><a href=""><i class="halflings-icon off"></i> Logout</a></li>
                            </ul>
                                -->
                            </li>
                            <!-- end: User Dropdown -->
                        </ul>
                    </div>
                    <!-- end: Header Menu -->

                </div>
            </div>
        </div>
        <!-- start: Header -->

        <div class="container-fluid-full">
            <div class="row-fluid">

                <!-- start: Main Menu -->
                <div id="sidebar-left" class="span2">
                    <div class="nav-collapse sidebar-nav">
                        <ul class="nav nav-tabs nav-stacked main-menu">
                            {% block menuLateral %}

                            {% endblock %}
                        </ul>
                    </div>
                </div>
                <!-- end: Main Menu -->

                <noscript>
                <div class="alert alert-block span10">
                    <h4 class="alert-heading">Warning!</h4>
                    <p>You need to have <a href="http://en.wikipedia.org/wiki/JavaScript" target="_blank">JavaScript</a> enabled to use this site.</p>
                </div>
                </noscript>

                <!-- start: Content -->
                <div id="content" class="span10">
                    {% block januxBread %}
                        {% include 'VTTTemplatesJanuxBundle:extras:bread-crumb.html.twig' %}
                    {% endblock %}

                    {% block body %}

                    {% endblock %}



                </div><!--/.fluid-container-->

                <!-- end: Content -->
            </div><!--/#content.span10-->
        </div><!--/fluid-row-->


        <div class="clearfix"></div>

        <footer>
            <p>
                <span style="text-align:left;float:left">&copy; 2013 <a href="http://themifycloud.com/downloads/janux-free-responsive-admin-dashboard-template/" alt="Bootstrap_Metro_Dashboard">JANUX Responsive Dashboard</a></span>
            </p>
        </footer>
        
        <span id="BASE_URL_VTT"></span>
        <span id="PATH_SOURCES_VTT"></span>
        <span id="FULL_URL_VTT"></span>

        <!-- start: JavaScript-->
        {% block javascript %}

            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery-1.9.1.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery-migrate-1.0.0.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery-ui-1.10.0.custom.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery.ui.touch-punch.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/modernizr.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/bootstrap.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery.cookie.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/fullcalendar.min.js') }}" type="text/javascript"></script>
            
            
            <script src="//cdn.datatables.net/1.10.5/js/jquery.dataTables.min.js"></script>
            
            
            <script src="{{ asset('bundles/vtttemplatesjanux/js/excanvas.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery.flot.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery.flot.pie.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery.flot.stack.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery.flot.resize.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery.chosen.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery.uniform.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery.cleditor.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery.noty.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery.raty.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery.iphone.toggle.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery.uploadify-3.1.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery.gritter.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery.imagesloaded.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery.masonry.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery.knob.modified.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery.sparkline.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/counter.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/retina.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/jquery.elfinder.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vtttemplatesjanux/js/custom.js') }}" type="text/javascript"></script>
            
        {% endblock %}
        
        
        {% block javascriptJsBundle %}
            <!-- Required -->
            <script src="{{ asset('js/plugins/blockui/jquery.blockUI.js')|raw }}"></script>
            
            <script src="{{ asset('bundles/vttjs/bootstrap_init.vtt.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vttjs/classJS/MensagemVTT/MensagemVTT.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vttjs/classJS/ModalVTT/ModalVTT.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vttjs/classJS/LoaderVTT/LoaderVTT.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vttjs/classJS/AjaxVTT/AjaxVTT.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vttjs/classJS/QuestionVTT/QuestionVTT.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/vttjs/classJS/DataTableVTT/DataTableVTT.js') }}" type="text/javascript"></script>
            
        {% endblock %}
        
        
        {% block javascriptLocal %}{% endblock %}


        <!-- end: JavaScript-->

    </body>
</html>