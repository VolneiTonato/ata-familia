<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<style>

    html, body{
        font-family:sans-serif;
        font-size:15px;
        
    }
    table{
        width: 100%;
        margin: auto auto;
    }
    
    th.principal{
        
    }

    .right{ text-align: right; }
    .center{ text-align: center; }
    .left{ text-align: left; }
    
    .titulo{
      background-color: silver !important;
      padding: 10px;
    }
    
    .titulo th h3{
        font-size: 25px;
    }
    
    td{
        height:40px;
        line-height:40px;
        font-size: 20px;
    }
    
    
    
</style>
{% if dados is defined and dados|length > 0 %}
    <table>
        <thead>
            <tr>
                <th class="principal" colspan="4"><h1>Planilha teste</h1></th>
            </tr>
            <tr class="titulo">
                <th class="center"><h3>Nome Completo</h3></th>
                <th class="center"><h3>Telefones</h3></th>
                <th class="center"><h3>Munic√≠pio</h3></th>
                <th class="center"><h3>Estado</h3></th>
            </tr>
        </thead>
        <tbody>
            {% for item in dados %}
            <tr class="body-tr">
                <td class="left">{{ item.nome }}</td>
                <td class="center">
                {% for fone in item.telefones %}
                    {{ fone.numero }} <br />
                {% endfor %}
                </td>
                <td class="center">{{ item.municipio.nome }}</td>
                <td class="center">{{ item.municipio.sigla }}</td>
            </tr>
            {% endfor %}
        </tbody>

    </table>
{% endif %}